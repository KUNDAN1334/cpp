// SORTING SET-PAIRS USING CPP LANGUAGE

#include<bits/stdc++.h>
using namespace std;

int main()
{
    vector<pair<int,int>> v;
    v.push_back({1,2});
    v.push_back({3,4});
    v.push_back({5,6});
    v.push_back({4,12});
    v.push_back({7,8});
    v.push_back({9,10});
    sort(v.begin(),v.end());

    for (int i=0;i<v.size();i++)
    {
        cout<<v[i].first<<",";
        cout<<v[i].second;
        cout<<endl;
    }
    cout<<endl<<v.size();

}
